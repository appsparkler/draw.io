  <diagram id="Nm3DymPliKEBzI2Rtd-W" name="Accessibility">
    <mxGraphModel dx="1062" dy="774" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="0" pageScale="1" pageWidth="850" pageHeight="1100" math="0" shadow="0">
      <root>
        <mxCell id="0" />
        <mxCell id="1" parent="0" />
        <mxCell id="fG6j18foGbW66ThMf-k3-15" value="" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;" parent="1" vertex="1">
          <mxGeometry x="-80" y="-240" width="320" height="280" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-1" value="If developers are unable to provide accessible experience, the disability is upon them." style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=3" parent="1" vertex="1">
          <mxGeometry x="-80" y="-100" width="320" height="30" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-3" value="&lt;div&gt;&lt;b&gt;Practicals:&lt;/b&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;- show billing address is same as shipping address checkbox&lt;/div&gt;&lt;div&gt;- avoid drag and drop or have alternate like - move to top, move up, etc.&lt;/div&gt;&lt;div&gt;- have large CTAs&lt;/div&gt;&lt;div&gt;&lt;div&gt;- use &lt;b&gt;sr-only &lt;/b&gt;class&lt;/div&gt;&lt;div&gt;- use &lt;b&gt;landmark tags&lt;/b&gt; - main, header, footer, nav, etc.&lt;br&gt;&lt;/div&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;align=left;verticalAlign=top;spacingTop=20;spacingLeft=8;spacingBottom=0;spacingRight=8;" parent="1" vertex="1">
          <mxGeometry x="120" y="120" width="160" height="240" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-4" value="&lt;b&gt;Help Pages&lt;/b&gt;&lt;br&gt;&lt;div&gt;- phone numbers and hours&lt;/div&gt;&lt;div&gt;- contact form&lt;/div&gt;&lt;div&gt;- email address&lt;/div&gt;&lt;div&gt;- chat bot&lt;/div&gt;&lt;div&gt;- human messaging/chat&lt;/div&gt;&lt;div&gt;- FAQ&lt;/div&gt;&lt;div&gt;- support/help pages&lt;/div&gt;&lt;div&gt;- social media channels&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;align=left;verticalAlign=top;spacingTop=20;spacingLeft=8;spacingBottom=0;spacingRight=8;" parent="1" vertex="1">
          <mxGeometry x="320" y="120" width="160" height="200" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-5" value="&lt;div&gt;&lt;b&gt;Process:&lt;/b&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;- ⭐️ check website in B/W mode&lt;/div&gt;&lt;div&gt;- use high-contrast ratio - for ex. 5:1 or more&lt;br&gt;&lt;/div&gt;&lt;div&gt;- use screen-readers such as Voice Over and NVDA to test the website&#39;s accessibility&lt;/div&gt;&lt;div&gt;- accessibility is done right all three stages of product development:&lt;br&gt;1. Design&lt;/div&gt;&lt;div&gt;2. Code&lt;/div&gt;&lt;div&gt;3. Content&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;- ⭐️ use indicators other than color - for ex. shapes, text, labels, patterns &lt;b&gt;- design phase&lt;/b&gt;&lt;/div&gt;&lt;div&gt;- avoid flashing or blinking content&lt;br&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;align=left;verticalAlign=top;spacingTop=20;spacingLeft=8;spacingBottom=0;spacingRight=8;" parent="1" vertex="1">
          <mxGeometry x="-160" y="120" width="240" height="280" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-6" value="&lt;div&gt;&lt;b&gt;Focus Indicator&lt;/b&gt;&lt;/div&gt;&lt;div&gt;- needs to be visible clearly on B/W mode of our website.&lt;/div&gt;&lt;div&gt;- should not be hidden behind other elements - for ex. by the chat box&lt;/div&gt;&lt;div&gt;- focused element should have solid outline&lt;/div&gt;&lt;div&gt;- focused v/s unfocused contrast &amp;gt; 3:1&lt;/div&gt;&lt;div&gt;- adjacent contrast &amp;gt; 3:1&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;align=left;verticalAlign=top;spacingTop=20;spacingLeft=8;spacingBottom=0;spacingRight=8;" parent="1" vertex="1">
          <mxGeometry x="520" y="120" width="160" height="240" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-7" value="Equity, not equality." style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=3" parent="1" vertex="1">
          <mxGeometry x="-80" y="-70" width="320" height="30" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-8" value="Use personas to step into the shoes of users" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=3" parent="1" vertex="1">
          <mxGeometry x="-80" y="-40" width="320" height="30" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-9" value="compliant !== accessible" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=3" parent="1" vertex="1">
          <mxGeometry x="-80" y="-10" width="320" height="30" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-11" value="Our application is accessible if it is:&lt;br&gt;- PERCEIVABLE&lt;br&gt;- OPERABLE&lt;br&gt;&lt;div&gt;- UNDERSTANDABLE&lt;br&gt;- ROBUST&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=3" parent="1" vertex="1">
          <mxGeometry x="-80" y="-200" width="320" height="100" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-12" value="&lt;div&gt;&lt;b&gt;ARIA&lt;/b&gt;&lt;/div&gt;&lt;div&gt;- &lt;b&gt;role=&quot;alert&quot;&lt;/b&gt; to publish any content that needs to be announced&lt;/div&gt;&lt;div&gt;- &lt;b&gt;aria-live=&quot;assertive&quot; &lt;/b&gt;to jump the queue and announce the contents&lt;/div&gt;&lt;div&gt;- use &lt;b&gt;aria-labelledby &lt;/b&gt;with &lt;b&gt;sr-only class &lt;/b&gt;instead of aria-label for translated content.&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;align=left;verticalAlign=top;spacingTop=20;spacingLeft=8;spacingBottom=0;spacingRight=8;" parent="1" vertex="1">
          <mxGeometry x="720" y="120" width="160" height="240" as="geometry" />
        </mxCell>
        <mxCell id="fG6j18foGbW66ThMf-k3-14" value="Example: &lt;b&gt;SR-ONLY with aria-labelledby&lt;br&gt;&lt;/b&gt;&lt;br&gt;&amp;lt;main aria-labelledby=&quot;page-title&quot;&amp;gt;&lt;br&gt;&lt;div&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&amp;lt;h1 id=&quot;page-title&quot; className=&quot;sr-only&quot;&amp;gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;Products&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&amp;lt;/h1&amp;gt;&lt;/div&gt;&amp;lt;/main&amp;gt;" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;align=left;spacing=16;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="920" y="120" width="240" height="160" as="geometry" />
        </mxCell>
        <mxCell id="xMTG2CMzGAB8xeqRqj2v-1" value="&lt;div&gt;&lt;b&gt;&amp;lt;img&amp;gt; tag&lt;br&gt;&lt;/b&gt;&lt;/div&gt;&lt;div&gt;- add &lt;b&gt;alt&lt;/b&gt; tag for informative images - for ex. for statistics, time-tables, etc.&lt;/div&gt;&lt;div&gt;- avoid them if they are purely decorative - for ex. backgrounds, etc.&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;align=left;verticalAlign=top;spacingTop=20;spacingLeft=8;spacingBottom=0;spacingRight=8;" vertex="1" parent="1">
          <mxGeometry x="1200" y="120" width="160" height="160" as="geometry" />
        </mxCell>
        <mxCell id="xMTG2CMzGAB8xeqRqj2v-2" value="&lt;div&gt;&lt;b&gt;&amp;lt;video&amp;gt; tag&lt;br&gt;&lt;/b&gt;&lt;/div&gt;&lt;div&gt;- add captions and transcriptions&lt;/div&gt;&lt;div&gt;- sign language interpretation where necessary.&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="rounded=1;whiteSpace=wrap;html=1;align=left;verticalAlign=top;spacingTop=20;spacingLeft=8;spacingBottom=0;spacingRight=8;" vertex="1" parent="1">
          <mxGeometry x="1400" y="120" width="200" height="130" as="geometry" />
        </mxCell>
      </root>
    </mxGraphModel>
