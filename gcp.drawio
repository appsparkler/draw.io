        <mxCell id="dxkdamIwOG48trZYYZ-_-1" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=10;fontStyle=1" value="&lt;div&gt;Essential GC Infra Foundation Course&lt;/div&gt;" vertex="1">
