        <mxCell id="mr_n5P6o90uTnWHHjz-A-10" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;rounded=0;fontSize=18;fontStyle=1" value="Managed Instance Groups" vertex="1">
