        </mxCell>
        <mxCell id="SQMnd0WqVCdwLsg4TDoG-12" parent="1" style="rounded=1;whiteSpace=wrap;html=1;absoluteArcSize=1;arcSize=14;strokeWidth=2;fillColor=#fff2cc;strokeColor=#d6b656;align=left;spacing=12;verticalAlign=top;dashed=1;" value="&lt;div&gt;&lt;b&gt;Roles&lt;/b&gt;&lt;/div&gt;&lt;div&gt;- define the &quot;can do what&quot; part of IAM&lt;/div&gt;&lt;div&gt;- there are three types:&lt;/div&gt;&lt;div&gt;&amp;nbsp; &lt;b&gt;i. Basic&lt;/b&gt;&amp;nbsp;&amp;nbsp;&lt;b&gt;ii. Predefined&amp;nbsp; iii. Custom&lt;/b&gt;&lt;/div&gt;" vertex="1">
          <mxGeometry height="90" width="260" x="550" y="480" as="geometry" />
        </mxCell>
        <mxCell id="SQMnd0WqVCdwLsg4TDoG-18" edge="1" parent="1" source="SQMnd0WqVCdwLsg4TDoG-14" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;exitX=0;exitY=0.5;exitDx=0;exitDy=0;" target="SQMnd0WqVCdwLsg4TDoG-16">
