        <mxCell id="2HWvdMSVWSpGX1TzIJff-17" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;rounded=0;fontStyle=1" value="No Ops" vertex="1">
