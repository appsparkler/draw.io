        <mxCell id="mr_n5P6o90uTnWHHjz-A-12" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;fontSize=19;fontStyle=1" value="Content Delivery Network" vertex="1">
