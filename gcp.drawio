        <mxCell id="gsbneUqQGO2pmt4NGT6P-5" parent="1" style="rounded=1;whiteSpace=wrap;html=1;absoluteArcSize=1;arcSize=14;strokeWidth=2;fillColor=#fff2cc;strokeColor=#d6b656;align=left;spacing=12;verticalAlign=top;dashed=1;" value="&lt;b&gt;The Tunnels&lt;br&gt;&lt;/b&gt;&lt;div&gt;- IPSec tunnel&lt;/div&gt;- traffic is encrypted by one gateway and decrypted by other VPN gateway" vertex="1">
          <mxGeometry height="100" width="280" x="-610" y="-1060" as="geometry" />
