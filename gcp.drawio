        <mxCell id="DCjnVqkURGrC5P2Fgk3D-27" parent="DCjnVqkURGrC5P2Fgk3D-22" style="shape=partialRectangle;html=1;whiteSpace=wrap;connectable=0;strokeColor=inherit;overflow=hidden;fillColor=none;top=0;left=0;bottom=0;right=0;pointerEvents=1;" value="" vertex="1" visible="0">
