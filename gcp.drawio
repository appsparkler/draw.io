        </mxCell>
        <mxCell id="GPO6o60PJQaoUWgeRMy--25" parent="1" style="rounded=1;whiteSpace=wrap;html=1;absoluteArcSize=1;arcSize=14;strokeWidth=2;fillColor=#fff2cc;strokeColor=#d6b656;align=left;spacing=12;verticalAlign=top;dashed=1;fontStyle=1" value="&lt;div&gt;&lt;font style=&quot;font-size: 16px;&quot;&gt;Serving state&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-weight: normal;&quot;&gt;- A container is in serving state as long as it handles web requests.&lt;/span&gt;&lt;/div&gt;" vertex="1">
          <mxGeometry height="90" width="210" x="350" y="1610" as="geometry" />
        </mxCell>
        <mxCell id="GPO6o60PJQaoUWgeRMy--26" edge="1" parent="1" source="GPO6o60PJQaoUWgeRMy--21" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;exitX=0.5;exitY=0;exitDx=0;exitDy=0;endArrow=oval;endFill=1;startArrow=oval;startFill=1;">
