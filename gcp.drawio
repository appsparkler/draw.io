        <mxCell id="gsbneUqQGO2pmt4NGT6P-47" parent="1" style="sketch=0;verticalLabelPosition=bottom;sketch=0;aspect=fixed;html=1;verticalAlign=top;strokeColor=none;fillColor=#000000;align=center;outlineConnect=0;pointerEvents=1;shape=mxgraph.citrix2.authentication_google;labelBackgroundColor=#FFFFFF;" value="" vertex="1">
